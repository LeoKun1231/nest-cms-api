{
  "name": "cms",
  "version": "0.0.1",
  "description": "",
  "author": "leo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development  ts-node src/index.ts",
    "start:dev": "cross-env NODE_ENV=development  nest start --watch",
    "start:debug": " nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production  node dist/src/main",
    "start:swc": "cross-env NODE_ENV=development nest start --builder swc --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "cross-env NODE_ENV=development pnpm  typeorm migration:generate -p -d ./src/ormconfig.ts migrations/%npm_config_name%",
    "migration:create": "cross-env NODE_ENV=development typeorm-ts-node-commonjs migration:create",
    "migration:run": "cross-env NODE_ENV=development pnpm  typeorm migration:run -d ./src/ormconfig.ts",
    "migration:revert": "cross-env NODE_ENV=development pnpm  typeorm migration:revert -d ./src/ormconfig.ts",
    "schema:drop": "cross-env NODE_ENV=development pnpm  typeorm schema:drop  -d ./src/ormconfig.ts",
    "prepare": "husky install",
    "fix": "eslint . --ext .js,.ts,--fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dompurify": "^3.0.6",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "jsdom": "^22.1.0",
    "mysql2": "^3.6.2",
    "nest-winston": "^1.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.17",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.95",
    "@types/bcrypt": "^5.0.1",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^16.11.10",
    "@types/passport-jwt": "^3.0.10",
